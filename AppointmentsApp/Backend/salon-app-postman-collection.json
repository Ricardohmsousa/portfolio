{
	"info": {
	  "name": "Flask API",
	  "_postman_id": "unique-id",
	  "description": "Collection for Flask API endpoints",
	  "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
	  {
		"name": "Register User",
		"request": {
		  "method": "POST",
		  "header": [
			{
			  "key": "Content-Type",
			  "value": "application/json"
			}
		  ],
		  "body": {
			"mode": "raw",
			"raw": "{\"email\": \"user@example.com\", \"password\": \"yourpassword\", \"username\": \"yourusername\"}"
		  },
		  "url": {
			"raw": "http://localhost:5000/register",
			"protocol": "http",
			"host": [
			  "localhost"
			],
			"port": "5000",
			"path": [
			  "register"
			]
		  }
		}
	  },
	  {
		"name": "Login User",
		"request": {
		  "method": "POST",
		  "header": [
			{
			  "key": "Content-Type",
			  "value": "application/json"
			}
		  ],
		  "body": {
			"mode": "raw",
			"raw": "{\"email\": \"user@example.com\", \"password\": \"yourpassword\"}"
		  },
		  "url": {
			"raw": "http://localhost:5000/login",
			"protocol": "http",
			"host": [
			  "localhost"
			],
			"port": "5000",
			"path": [
			  "login"
			]
		  }
		}
	  },
	  {
		"name": "Create Salon",
		"request": {
		  "method": "POST",
		  "header": [
			{
			  "key": "Content-Type",
			  "value": "application/json"
			},
			{
			  "key": "Authorization",
			  "value": "Bearer <your_jwt_token>"
			}
		  ],
		  "body": {
			"mode": "raw",
			"raw": "{\"name\": \"Salon Name\", \"address\": \"Salon Address\", \"phone\": \"123-456-7890\"}"
		  },
		  "url": {
			"raw": "http://localhost:5000/salon",
			"protocol": "http",
			"host": [
			  "localhost"
			],
			"port": "5000",
			"path": [
			  "salon"
			]
		  }
		}
	  },
	  {
		"name": "Get Salon",
		"request": {
		  "method": "GET",
		  "header": [
			{
			  "key": "Authorization",
			  "value": "Bearer <your_jwt_token>"
			}
		  ],
		  "url": {
			"raw": "http://localhost:5000/salon/<id>",
			"protocol": "http",
			"host": [
			  "localhost"
			],
			"port": "5000",
			"path": [
			  "salon",
			  "<id>"
			]
		  }
		}
	  },
	  {
		"name": "Get Available Slots",
		"request": {
		  "method": "GET",
		  "header": [
			{
			  "key": "Authorization",
			  "value": "Bearer <your_jwt_token>"
			}
		  ],
		  "url": {
			"raw": "http://localhost:5000/available_slots?salon_id=<salon_id>&service_id=<service_id>&date=<YYYY-MM-DD>",
			"protocol": "http",
			"host": [
			  "localhost"
			],
			"port": "5000",
			"path": [
			  "available_slots"
			],
			"query": [
			  {
				"key": "salon_id",
				"value": "<salon_id>"
			  },
			  {
				"key": "service_id",
				"value": "<service_id>"
			  },
			  {
				"key": "date",
				"value": "<YYYY-MM-DD>"
			  }
			]
		  }
		}
	  }
	]
  }
  